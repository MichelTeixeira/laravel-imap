{
    "name": "rgiordano/laravel-imap",
    "type": "library",
    "description": "Lumen version of Laravel IMAP client",
    "keywords": [
        "webklex",
        "laravel",
        "imap",
        "laravel-imap",
        "mail"
    ],
    "homepage": "https://github.com/webklex/laravel-imap",
    "license": "MIT",
    "authors": [
        {
            "name": "Malte Goldenbaum",
            "email": "github@webklex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": ">=5.0.0"
    },
    "autoload": {
        "psr-4": {
            "Rgiordano\\IMAP\\": "src/IMAP"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Rgiordano\\IMAP\\Providers\\LaravelServiceProvider"
            ],
            "aliases": {
                "Client": "Rgiordano\\IMAP\\Facades\\Client"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
